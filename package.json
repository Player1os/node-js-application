{
  "name": "@player1os/ts-node-application-template",
  "version": "0.0.0",
  "description": "A template project from which NodeJS applications written in Typescript can be derived",
  "scripts": {
    "preversion": "support-require @player1os/typescript-support/lib/task/preversion",
    "postversion": "support-require @player1os/typescript-support/lib/task/postversion",
    "update-base-create": "support-require @player1os/typescript-support/lib/task/update_base/create",
    "update-base-load": "support-require @player1os/typescript-support/lib/task/update_base/load",
    "publish": "echo 'Error: no publish script specified'",
    "start": "node --nolazy --inspect-brk build/index.js",
    "build": "support-require @player1os/typescript-support/lib/task/_build",
    "build-watch": "support-require @player1os/typescript-support/lib/task/_build/watch",
    "test": "support-require @player1os/typescript-support/lib/task/_test",
    "test-watch": "support-require @player1os/typescript-support/lib/task/_test/watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Player1os/ts-node-application-template.git"
  },
  "keywords": [
    "typescript",
    "node",
    "application",
    "template"
  ],
  "author": "Osama Hassanein <player1os@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Player1os/ts-node-application-template/issues"
  },
  "homepage": "https://github.com/Player1os/ts-node-application-template/#readme",
  "engines": {
    "node": "^10.6.0",
    "npm": "^6.1.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "dotenv": "^6.0.0",
    "raven": "^2.6.3",
    "source-map-support": "^0.5.9"
  },
  "devDependencies": {
    "@player1os/typescript-support": "^1.3.9",
    "@types/node": "^12.11.2",
    "@types/raven": "^2.5.1",
    "coveralls": "^3.0.2"
  }
}
